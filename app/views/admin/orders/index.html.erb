<div class="container">
 	<div class="row">
 		
 		<div class="col-md-10 col-md-offset-1">
 			<h4>
 				<span>注文履歴一覧</span>
 			</h4>
 		</div>

 		<div class="admin_orders_table col-md-10 col-md-offset-1">
 			<table class="table">
 				<thead>
 					<tr>
 						<th>購入日時</th>
 						<th>購入者</th>
 						<th></th>
 						<th></th>
 					</tr>
 				</thead>
 				<tbody>
 					<% @orders.each do |order| %>
 					<tr>
 						<td><%= link_to order.created_at.strftime('%Y/%m/%d %H:%M:%S'), admin_order_path(order.id) %></td>
 						<td><%= order.end_user.full_name %></td>
 						<td></td>
 						<td></td>
 					</tr>
 					<% end %>
 				</tbody>
 			</table>
 		</div>
 	</div>
</div>
