<div class="container">
 	<div class="row">

 		<div class="col-md-10 col-md-offset-1">
 			<h4>
 				<span><%= @end_user.full_name %>の会員詳細</span>
 			</h4>
 		</div>

 		<div class="col-md-5">

	            <table class="table">
	              <tbody>
	                <tr>
	                  <th>顧客ID</th>
	                  <td>
	                      <%= @end_user.id %>
	                  </td>
	                </tr>
	                  <th>氏名</th>
	                  <td>
	                      <%= @end_user.last_name %>
	                      <%= @end_user.first_name%>
	                  </td>
	                </tr>
	                <tr>
	                  <th>フリガナ</th>
	                  <td>
	                      <%= @end_user.last_name_kana %>
	                      <%= @end_user.first_name_kana %>
	                  </td>
	                </tr>
	                <tr>
	                  <th>郵便番号</th>
	                  <td><%= @end_user.zipcord %></td>
	                </tr>
	                <tr>
	                  <th>住所</th>
	                  <td><%= @end_user.address %></td>
	                </tr>
	                <tr>
	                  <th>電話番号</th>
	                  <td><%= @end_user.phone_number %></td>
	                </tr>
	                <tr>
	                  <th>メールアドレス</th>
	                  <td><%= @end_user.email %></td>
	                </tr>

	              </tbody>
	            </table>
	    </div>

	    <div class="col-md-12">

	    <%= link_to '編集する', edit_admin_user_path(@end_user.id), class: "btn btn-primary btn-md" %>

	    <%= link_to '注文履歴一覧へ', admin_orders_path, class: "btn btn-primary btn-md" %>

	   </div>



	</div>
</div>